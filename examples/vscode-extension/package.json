{
  "name": "Enigma AI Engine-vscode",
  "displayName": "Enigma AI Engine",
  "description": "AI-powered coding assistant powered by Enigma AI Engine",
  "version": "1.0.0",
  "publisher": "Enigma AI Engine",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Machine Learning",
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "ai",
    "assistant",
    "code generation",
    "autocomplete",
    "forge"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Enigma AI Engine/Enigma AI Engine-vscode"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Enigma AI Engine.chat",
        "title": "Enigma AI Engine: Open Chat"
      },
      {
        "command": "Enigma AI Engine.explainCode",
        "title": "Enigma AI Engine: Explain Code"
      },
      {
        "command": "Enigma AI Engine.generateCode",
        "title": "Enigma AI Engine: Generate Code"
      },
      {
        "command": "Enigma AI Engine.refactorCode",
        "title": "Enigma AI Engine: Refactor Code"
      },
      {
        "command": "Enigma AI Engine.addComments",
        "title": "Enigma AI Engine: Add Comments"
      },
      {
        "command": "Enigma AI Engine.generateTests",
        "title": "Enigma AI Engine: Generate Tests"
      },
      {
        "command": "Enigma AI Engine.findBugs",
        "title": "Enigma AI Engine: Find Bugs"
      },
      {
        "command": "Enigma AI Engine.optimizeCode",
        "title": "Enigma AI Engine: Optimize Code"
      },
      {
        "command": "Enigma AI Engine.askAboutFile",
        "title": "Enigma AI Engine: Ask About This File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "Enigma AI Engine.explainCode",
          "when": "editorHasSelection",
          "group": "Enigma AI Engine@1"
        },
        {
          "command": "Enigma AI Engine.refactorCode",
          "when": "editorHasSelection",
          "group": "Enigma AI Engine@2"
        },
        {
          "command": "Enigma AI Engine.addComments",
          "when": "editorHasSelection",
          "group": "Enigma AI Engine@3"
        },
        {
          "command": "Enigma AI Engine.generateTests",
          "when": "editorHasSelection",
          "group": "Enigma AI Engine@4"
        },
        {
          "command": "Enigma AI Engine.findBugs",
          "when": "editorHasSelection",
          "group": "Enigma AI Engine@5"
        }
      ]
    },
    "keybindings": [
      {
        "command": "Enigma AI Engine.chat",
        "key": "ctrl+shift+f",
        "mac": "cmd+shift+f"
      },
      {
        "command": "Enigma AI Engine.generateCode",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      },
      {
        "command": "Enigma AI Engine.explainCode",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e",
        "when": "editorHasSelection"
      }
    ],
    "configuration": {
      "title": "Enigma AI Engine",
      "properties": {
        "Enigma AI Engine.apiUrl": {
          "type": "string",
          "default": "http://localhost:8000",
          "description": "URL of the Enigma AI Engine API server"
        },
        "Enigma AI Engine.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for authentication (optional)"
        },
        "Enigma AI Engine.model": {
          "type": "string",
          "default": "default",
          "description": "Model to use for code generation"
        },
        "Enigma AI Engine.maxTokens": {
          "type": "number",
          "default": 2048,
          "description": "Maximum tokens in response"
        },
        "Enigma AI Engine.temperature": {
          "type": "number",
          "default": 0.7,
          "minimum": 0,
          "maximum": 2,
          "description": "Temperature for generation (0-2)"
        },
        "Enigma AI Engine.inlineCompletions": {
          "type": "boolean",
          "default": true,
          "description": "Enable inline code completions"
        },
        "Enigma AI Engine.autoSuggest": {
          "type": "boolean",
          "default": false,
          "description": "Automatically suggest improvements"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "Enigma AI Engine",
          "title": "Enigma AI Engine",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "Enigma AI Engine": [
        {
          "id": "Enigma AI Engine.chat",
          "name": "Chat",
          "type": "webview"
        },
        {
          "id": "Enigma AI Engine.history",
          "name": "History"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint src --ext ts",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "@vscode/test-electron": "^2.3.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
